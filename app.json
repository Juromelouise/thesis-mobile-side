{
  "expo": {
    "name": "BOVO",
    "platforms": ["ios", "android", "web"],
    "slug": "BOVO",
    "assetBundlePatterns": ["**/*"],
    "version": "1.0.0",
    "plugins": [
      ["@react-native-google-signin/google-signin"],
      [
        "expo-notifications",
        {
          "icon": "./assets/BOVO_logo.png"
        }
      ],
      "expo-asset",
    ],
    "orientation": "portrait",
    "icon": "./assets/BOVO_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/BOVO_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tupt.bovo"
    },
    "android": {
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/BOVO_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tupt.bovo",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDbeRxVmUVGZoqB6aqIk8WggFh7wyzHo4k"
        }
      }
    },
    "web": {
      "favicon": "./assets/BOVO_logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "866a97ec-6522-48ed-98e4-e8a4e3512b43"
      }
    }
  }
}
